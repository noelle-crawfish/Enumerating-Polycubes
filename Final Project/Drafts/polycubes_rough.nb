Cell[BoxData[
 RowBox[{
  RowBox[{"randomCubes", "[", "n_", "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"points", " ", "=", " ", 
     RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"While", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Length", "@", "points"}], " ", "<", " ", "n"}], ",", " ", 
      RowBox[{"(", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"p", " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"RandomInteger", "[", "n", "]"}], ",", " ", 
           RowBox[{"RandomInteger", "[", "n", "]"}], ",", " ", 
           RowBox[{"RandomInteger", "[", "n", "]"}]}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"(", 
            RowBox[{"p", " ", "//", " ", 
             RowBox[{"MemberQ", "[", "points", "]"}]}], ")"}]}], ",", " ", 
          RowBox[{"points", " ", "=", " ", 
           RowBox[{"points", " ", "//", " ", 
