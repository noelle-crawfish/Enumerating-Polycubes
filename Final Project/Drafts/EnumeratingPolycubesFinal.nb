          RowBox[{
           RowBox[{
            RowBox[{"Length", "@", "npc"}], " ", ">", " ", "0"}], " ", "||", 
           " ", "processing"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"processing", " ", "=", " ", "False"}], ";", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", 
           RowBox[{"(*", " ", "NPC", " ", "*)"}], "\[IndentingNewLine]", 
